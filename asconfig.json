{
  "targets": {
    "debug": {
      "outFile": "build/debug.wasm",
      "textFile": "build/debug.wat",
      "disable": "bulk-memory",
      "exportStart": "init",
      "debug": true,
      "stats": true
    },
    "release": {
      "outFile": "build/release.wasm",
      "textFile": "build/release.wat",
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false,
      "disable": "bulk-memory",
      "exportStart": "init",
      "stats": true
    }
  }
}
